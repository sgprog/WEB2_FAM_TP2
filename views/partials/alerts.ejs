


<% if(typeof errList != 'undefined') { %>

    <% errList.forEach(function(error) {%>
      
        <div class="alert alert-warning alert-dismissible fade-web2-tp1 show" role="alert">
            <%= error.msg %>
         
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>        
    <% }); %>
<% } %>

<% if(success_msg != '') { %>
    <div class="alert alert-success alert-dismissible fade-web2-tp1 show" role="alert">
        <%= success_msg %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>      
<% } %>

<% if(error_msg != '') { %>
    <div class="alert alert-warning alert-dismissible fade-web2-tp1 show" role="alert">
        <%= error_msg %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>      
<% } %>

<% if(error_passport != '') { %>
    <div class="alert alert-warning alert-dismissible fade-web2-tp1 show" role="alert">
        <%= error_passport %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>      
<% } %>